		{
			"name": "Create Booking- Add Testcases Using AI",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Booking ID is not null, not zero, and is an integer\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    const bookingId = responseJson.bookingid;\r",
							"\r",
							"    pm.expect(bookingId).to.exist.and.to.not.be.null;\r",
							"    pm.expect(bookingId).to.not.equal(0);\r",
							"    pm.expect(bookingId).to.be.a('number').and.to.satisfy(Number.isInteger);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 500ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});\r",
							"pm.test(\"Bookingdates checkin date is a valid date\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    const checkinDate = new Date(responseJson.booking.bookingdates.checkin);\r",
							"    pm.expect(checkinDate.getTime()).to.be.a('number').that.is.not.NaN;\r",
							"});\r",
							"\r",
							"pm.test(\"Bookingdates checkout date is a valid date\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    const checkoutDate = new Date(responseJson.booking.bookingdates.checkout);\r",
							"    pm.expect(checkoutDate.getTime()).to.be.a('number').that.is.not.NaN;\r",
							"});\r",
							"pm.test(\"Response has Etag header\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Etag\")).to.exist;\r",
							"});\r",
							"\r",
							"pm.test(\"Response has valid Content-Length header\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Length\")).to.exist;\r",
							"    pm.expect(parseInt(pm.response.headers.get(\"Content-Length\"))).to.be.above(0);\r",
							"});\r",
							"\r",
							"pm.test(\"Response has Content-Type header\", function () {\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.exist;\r",
							"});\r",
							"\r",
							"pm.test(\"Response body has valid structure\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson).to.have.property(\"bookingid\");\r",
							"    pm.expect(responseJson).to.have.property(\"booking\");\r",
							"});\r",
							"\r",
							"pm.test(\"Booking contains valid firstname\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.property(\"firstname\", \"Jim\");\r",
							"});\r",
							"\r",
							"pm.test(\"Booking contains valid lastname\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.property(\"lastname\", \"Brown\");\r",
							"});\r",
							"\r",
							"pm.test(\"Booking contains valid totalprice\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.property(\"totalprice\", 111);\r",
							"});\r",
							"\r",
							"pm.test(\"Booking contains valid depositpaid\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.property(\"depositpaid\", true);\r",
							"});\r",
							"\r",
							"pm.test(\"Bookingdates has valid checkin date\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.nested.property(\"bookingdates.checkin\", \"2018-01-01\");\r",
							"});\r",
							"\r",
							"pm.test(\"Bookingdates has valid checkout date\", function () {\r",
							"    const responseJson = pm.response.json();\r",
							"    pm.expect(responseJson.booking).to.have.nested.property(\"bookingdates.checkout\", \"2019-01-01\");\r",
